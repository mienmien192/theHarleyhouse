<div class="container " style="margin-top: 150px;">
    <center>
        <h1 style="color: green">Danh sách khách hàng</h1>
    </center>
    <hr>
    <%- include('../part/message') %>
        <table class="table table-light table-striped">
            <tr>
                <th>Tên khách hàng</th>

                <th>Email</th>

                <th>Mật khẩu</th>
                <th>Sửa</th>
                <th>Xóa</th>

            </tr>
            <%
        users.map(user=>{%>
                <tr>
                    <td>
                        <%= user.username %>
                    </td>
                    <td>
                        <%= user.email %>
                    </td>
                    <td>
                        <%= user.password %>
                    </td>
                    <td><a href="/" class="btn btn-outline-success"><i class="fa fa-close"></i></a></td>
                    <td>
                        <%- include('../part/deleteForm',{url:`/user/delete/${user._id}?_method=DELETE`}) %>
                    </td>
                </tr>
                <%})
%>
        </table>
</div>